### VariablenLIC_BISF_CLI_RSS 	Disable Receive Side Scaling (RSS)LIC_BISF_CLI_DEP 	Disable Data Execution Prevention### 97_PrepBISF_BaseImage.ps1	$PrepCommands += [pscustomobject]@{		Order       = "$ordercnt";		Enabled     = "$true";    showmessage = "Y";		CLI         = "LIC_BISF_CLI_RSS";		TestPath    = "";		Description = "Disable Receive Side Scaling (RSS)";		Command     = "Start-Process -FilePath 'netsh.exe' -Argumentlist 'int tcp set global rss=disable' -Wait -WindowStyle Hidden"	};	$ordercnt += 1	$PrepCommands += [pscustomobject]@{		Order       = "$ordercnt";		Enabled     = "$true";		showmessage = "Y";		CLI         = "LIC_BISF_CLI_DEP";		TestPath    = "";		Description = "Disable Data Execution Prevention";		Command     = "Start-Process 'bcdedit.exe' -Verb runAs -ArgumentList '/set nx AlwaysOff' | Out-Null"	};	$ordercnt += 1###ADMX File    <policy name="POL_DEP" class="Machine" displayName="$(string.POL_DEP)" explainText="$(string.POL_DEP_Help)" key="SOFTWARE\Policies\Login Consultants\BISF" valueName="LIC_BISF_POL_DEP">      <parentCategory ref="CAT_MS" />      <supportedOn ref="windows:SUPPORTED_WindowsVista" />      <enabledList>        <item key="SOFTWARE\Policies\Login Consultants\BISF" valueName="LIC_BISF_CLI_DEP">          <value>            <string>YES</string>          </value>        </item>      </enabledList>      <disabledList>        <item key="SOFTWARE\Policies\Login Consultants\BISF" valueName="LIC_BISF_CLI_DEP">          <value>            <string>NO</string>          </value>        </item>      </disabledList>    </policy>    <policy name="POL_RSS" class="Machine" displayName="$(string.POL_RSS)" explainText="$(string.POL_RSS_Help)" key="SOFTWARE\Policies\Login Consultants\BISF" valueName="LIC_BISF_POL_RSS">      <parentCategory ref="CAT_MS" />      <supportedOn ref="windows:SUPPORTED_WindowsVista" />      <enabledList>        <item key="SOFTWARE\Policies\Login Consultants\BISF" valueName="LIC_BISF_CLI_RSS">          <value>            <string>YES</string>          </value>        </item>      </enabledList>      <disabledList>        <item key="SOFTWARE\Policies\Login Consultants\BISF" valueName="LIC_BISF_CLI_RSS">          <value>            <string>NO</string>          </value>        </item>      </disabledList>    </policy>   ## ADML File             <string id="POL_RSS">Disable Receive Side Scaling (RSS)</string>      <string id="POL_RSS_Help">This policy setting allows you to disable RSS on your base image disk (Citrix PVS or MCS) or your local system drive (VMware View or Microsoft Windows).If you enable this policy setting, RSS will be disabled.If you disable or do not configure this policy setting, RSS will not be touched.</string>         <string id="POL_DEP">Disable Data Execution Prevention</string>      <string id="POL_DEP_Help">This policy setting allows you to disable Data Execution Prevention on your base image disk (Citrix PVS or MCS) or your local system drive (VMware View or Microsoft Windows).If you enable this policy setting, Data Execution Prevention will be disabled.If you disable or do not configure this policy setting, Data Execution Prevention will not be touched.</string> ###FIX default Value "        "      <presentation id="POL_CTXOE">        <textBox refId="TXT_CTXOE_TemplatePath">          <label>(Optional) Template:</label>          <defaultValue></defaultValue>        </textBox>        <textBox refId="TXT_CTXOE_Groups">          <label>(Optional) List of enabled Groups:</label>          <defaultValue></defaultValue>        </textBox>###879		IF ( (($prepCommand.TestPath -eq "" ) -or (Test-Path $($prepCommand.TestPath))) -and $prepCommand.Enabled -eq "$true" ) {